{
  "manifest_version": 2,

  "name": "M-Study 2",
  "description": "This extension records audio and converts it to text",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "notifications",
    "tabs",
    "create"
  ]
}
