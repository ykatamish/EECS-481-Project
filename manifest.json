{
	"manifest_version": 2,
	"name": "M-Study 2",
	"description": "Convert audio files to text.",
	"version": "1.0",
	"permissions": [
		"storage"
	],
	"icons": { 
		"16": "img/icon.png",
		"48": "img/icon.png",
		"128": "img/icon.png" 
	},
	"browser_action": {
		"default_icon": {                    
			"19": "img/icon48.png",          
			"38": "img/icon48.png"          
		},
		"default_popup": "popup.html"
	},
	"web_accessible_resources": [
		"img/icon128.png"
	]
}